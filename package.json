{
  "name": "CSSModulesExample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "tsc": "tsc --project . --noEmit",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint": "eslint . && stylelint 'src/**/*.@(css|scss)'",
    "web": "webpack-dev-server --mode development",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "es6-shim": "^0.35.6",
    "react": "16.13.1",
    "react-art": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-fontawesome": "^1.6.1",
    "react-native": "0.63.4",
    "react-native-fontawesome": "5.7.0",
    "react-native-web": "^0.12.2",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.6",
    "@babel/runtime": "^7.14.6",
    "@react-native-community/eslint-config": "^3.0.0",
    "@teamthread/strict-css-modules-loader": "^0.0.3",
    "@types/jest": "^24.0.13",
    "@types/prop-types": "^15.7.1",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-native": "kristerkari/react-native-types-for-css-modules#v0.63.52",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/parser": "^4.28.2",
    "autoprefixer": "^9.5.1",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-react-native-classname-to-style": "^1.2.2",
    "babel-plugin-react-native-platform-specific-extensions": "^1.1.1",
    "babel-preset-react": "^7.0.0-beta.3",
    "css-loader": "1.0.1",
    "dts-css-modules-loader": "^1.2.3",
    "eslint": "^7.30.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "file-loader": "^3.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "node-sass": "^7.0.0",
    "patch-package": "^6.2.1",
    "postcss": "^8.3.5",
    "postcss-css-variables": "^0.18.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.3.2",
    "react-hot-loader": "3.1.3",
    "react-native-sass-transformer": "^1.4.0",
    "react-native-typed-postcss-transformer": "^0.20.9",
    "react-test-renderer": "16.13.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "10.0.1",
    "stylelint-config-react-native-css-modules": "^3.0.0",
    "stylelint-react-native": "^2.1.1",
    "ts-loader": "^6.2.2",
    "typescript": "^3.9.10",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11",
    "Android >= 4.4",
    "iOS >= 8"
  ],
  "jest": {
    "preset": "react-native",
    "verbose": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "testMatch": [
      "**/__tests__/**/*.(js|ts)?(x)",
      "**/?(*.)+(spec|test).(js|ts)?(x)"
    ],
    "transformIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/?!(react-native)"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)?$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    }
  },
  "postcss": {
    "plugins": {
      "postcss-css-variables": {}
    }
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2018,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "jest": true
    },
    "plugins": [
      "css-modules"
    ],
    "extends": [
      "@react-native-community",
      "plugin:css-modules/recommended"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-react-native-css-modules",
    "rules": {
      "selector-class-pattern": "^[a-z][a-zA-Z0-9]*$",
      "declaration-block-no-duplicate-properties": true,
      "no-duplicate-selectors": true,
      "no-extra-semicolons": true,
      "no-eol-whitespace": true,
      "no-missing-end-of-source-newline": true
    }
  }
}
